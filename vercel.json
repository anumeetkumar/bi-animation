{
    "version": 2,
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "style.css",
        "use": "@vercel/static"
      },
      {
        "src": "script.js",
        "use": "@vercel/static"
      },
      {
        "src": "assets/(.*)",
        "use": "@vercel/static"
      }
    ]
  }
  